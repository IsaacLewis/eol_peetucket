<% blind_outlink_parameter=URI::escape(remove_html(@taxon.name(current_user.expertise, current_user.language))) %>
<div class="cpc-header" >
    <h3><%= TocItem.search_the_web.label %></h3>
</div>
<!-- /center page content header -->

<!-- center page content main content -->
<div class="cpc-content" >

  <%=render :partial=>'/shared/unknown_text_warning_box' %>

  <div class="unvetted-background-text" id="links">
    <table cellspacing="0" summary="Links" class="results_table">
      <tbody><tr><th>Item</th><th>Description</th></tr>
        <tr><td width="100" height="60" class="odd"><%=external_link_to(image_tag('agent_icons/google.gif'),'http://www.google.com/search?q='+blind_outlink_parameter,{:show_link_icon => false})%></td><td class="odd"><%=external_link_to("Google","http://www.google.com/search?q="+blind_outlink_parameter,:show_link_icon=>true)%></td></tr>
        <tr><td width="100" height="60" class="even"><%=external_link_to(image_tag('agent_icons/google.gif'),'http://images.google.com/images?q='+blind_outlink_parameter,{:show_link_icon => false})%></td><td class="even"><%=external_link_to("Google Images","http://images.google.com/search?q="+blind_outlink_parameter,:show_link_icon=>true)%></td></tr>
        <tr><td width="100" height="60" class="odd"><%=external_link_to(image_tag('agent_icons/google.gif'),'http://scholar.google.com/scholar?q='+blind_outlink_parameter,{:show_link_icon => false})%></td><td class="odd"><%=external_link_to("Google Scholar","http://scholar.google.com/scholar?q="+blind_outlink_parameter,:show_link_icon=>true)%></td></tr>
        <tr><td width="100" height="60" class="even"><%=external_link_to(image_tag('agent_icons/yahoo.gif'),'http://search.yahoo.com/search?p='+blind_outlink_parameter,{:show_link_icon => false})%></td><td class="even"><%=external_link_to("Yahoo","http://search.yahoo.com/search?p="+blind_outlink_parameter,:show_link_icon=>true)%></td></tr>
        <tr><td width="100" height="60" class="odd"><%=external_link_to(image_tag('agent_icons/yahoo.gif'),'http://images.search.yahoo.com/search/images?p='+blind_outlink_parameter,{:show_link_icon => false})%></td><td class="odd"><%=external_link_to("Yahoo Images","http://images.search.yahoo.com/search/images?p="+blind_outlink_parameter,:show_link_icon=>true)%></td></tr>
        <tr><td width="100" height="60" class="even"><%=external_link_to(image_tag('agent_icons/vivisimo.gif'),'http://vivisimo.com/vivisimo/cgi-bin/query-meta?query='+blind_outlink_parameter,{:show_link_icon => false})%></td><td class="even"><%=external_link_to("Vivisimo","http://vivisimo.com/vivisimo/cgi-bin/query-meta?query="+blind_outlink_parameter,:show_link_icon=>true)%></td></tr>
      </tbody>
    </table>
  </div>
  
</div>
